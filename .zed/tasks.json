[
  {
    "label": "Build JSON Templates",
    "command": "whiskers zed-icons.tera"
  },
  {
    "label": "Clean JSON Templates",
    "command": "rm icon_themes/*.json"
  },
  {
    "label": "Init Submodule",
    "command": "git submodule update --init --recursive"
  },
  {
    "label": "Build Icons",
    "command": "uv run python src/generate.py"
  },
  {
    "label": "Test All Icons",
    "command": "uv run python src/test.py"
  },
  {
    "label": "Test & Validate Defined Icons",
    "command": "uv run src/test.py --validate"
  }
]
